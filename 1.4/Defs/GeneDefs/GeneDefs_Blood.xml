<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>VRE_BlueBlood</defName>
		<label>blue blood</label>
		<displayCategory>Healing</displayCategory>
		<description>Carriers of this gene have low levels of certain minerals in their blood, which results in it having a dark blue color.</description>
		<iconPath>UI/Icons/Genes/Gene_BlueBlood</iconPath>
		<displayOrderInCategory>0</displayOrderInCategory>
		
		<exclusionTags>
			<li>AG_BloodType</li>
			<li>BloodType</li>
			<li>Blood</li>
		</exclusionTags>
		<biostatCpx>1</biostatCpx>

		<symbolPack>
			<wholeNameSymbols>
				<li>
					<symbol>bluebleeder</symbol>
				</li>

			</wholeNameSymbols>
			<prefixSymbols>
				<li>
					<symbol>blue</symbol>
				</li>
				<li>
					<symbol>aqua</symbol>
				</li>

			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>marine</symbol>
				</li>

			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">

				<customBloodThingDef>VRE_Filth_BlueBlood</customBloodThingDef>
				<customBloodIcon>UI/Icons/Medical/VRE_BlueBleeding</customBloodIcon>
				<customBloodEffect>VRE_Damage_HitBlueFlesh</customBloodEffect>
				<customWoundsFromFleshtype>VRE_BlueWoundsFlesh</customWoundsFromFleshtype>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>